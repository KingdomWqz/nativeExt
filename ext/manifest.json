{
    "manifest_version": 2,
    "name": "Native Extension",
    "short_name": "nativeExt",
    "version": "0.1.0",
    "description": "原生应用连接插件",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "tabs",
      "activeTab",
      "background",
      "natvieMessage",
      "<all_urls>"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }